<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@model.title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.0/css/bootstrap.min.css" integrity="sha512-P5MgMn1jBN01asBgU0z60Qk4QxiXo86+wlFahKrsQf37c9cro517WzVSPPV1tDKzhku2iJ2FVgL67wG03SGnNA==" crossorigin="anonymous" />
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <div class="container-fluid">
      <div class="row mx-auto justify-content-center">
        <div class="col-8">
          <table class="table table-hover">
            <thead>
              <tr>

              </tr>
            </thead>
            <tbody>

            </tbody>
          </table>
        </div>
        @{var sno = 0;}
        <div class="col-4">
          <form method="POST" class="student-dataupdate" action="student/save">
            <table class="table table-bordered">
              <thead>
                <tr>
                  <th scope="col" colspan="2">Reg #:</th>
                  <th scope="col" colspan="2"></th>
                </tr>
                <tr>
                  <th scope="col" colspan="2">Name:</th>
                  <th scope="col" colspan="2"></th>
                </tr>
                <tr>
                  <th scope="col">#</th>
                  <th scope="col">Head</th>
                  <th scope="col">Total</th>
                  <th scope="col">Marks</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="5"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="5"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="5"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="5"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="35"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="25"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="15"></td>
                </tr>
                <tr>
                  <th scope="row">@(++sno)</th>
                  <td></td>
                  <td></td>
                  <td><input class="form-control" type="number" min="0" max="5"></td>
                </tr>
                <tr>
                  <td colspan="3">Total</td>
                  <td></td>
                </tr>
                <tr>
                  <td colspan="3">Percentage</td>
                  <td></td>
                </tr>
                <tr>
                  <td colspan="3">Grade</td>
                  <td></td>
                </tr>
              </tbody>
            </table>
            @{sno = 0}
          </form>
          <h4 class="text-danger text-uppercase"></h4>
        </div>
      </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.0/js/bootstrap.min.js" integrity="sha512-XKa9Hemdy1Ui3KSGgJdgMyYlUg1gM+QhL6cnlyTe2qzMCYm4nAZ1PsVerQzTTXzonUR+dmswHqgJPuwCq1MaAg==" crossorigin="anonymous"></script>
    <script src="/javascripts/custom.js" async defer></script>
  </body>
</html>